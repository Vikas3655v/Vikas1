#include<stdio.h>
  
void main ()  
{  
    int n, r, per, fact1, fact2,num,i;  
    printf("nPr calculation");
    scanf("%d%d",&n,&r);  
    fact1 = n;  
    for (int i = n - 1; i >= 1; i--)  
    {  
        fact1 = fact1 * i;  
    }  
    num = n - r;  
    fact2 = num;  
    for (i = num - 1; i >= 1; i--)  
    {  
        fact2 = fact2 * i;  
    }  
    per = fact1 / fact2;  
    printf("nPr = %d",per);  
      }  
